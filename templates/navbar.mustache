<div class="row pt-3 mb-3">
    <div class="col">
        <ul class="nav nav-pills" role="menubar">
            <li class="nav-item" role="none">
                <a href="{{myurl}}" title="{{#str}}navbar:tab:myeportfolios, local_eportfolio{{/str}}"
                   class="nav-link {{myactive}}"
                   role="menuitem" {{myariaselected}}>
                    {{#str}}navbar:tab:myeportfolios, local_eportfolio{{/str}}
                </a>
            </li>
            <li class="nav-item" role="none">
                <a href="{{mysharedurl}}" title="{{#str}}navbar:tab:mysharedeportfolios, local_eportfolio{{/str}}"
                   class="nav-link {{mysharedactive}}"
                   role="menuitem" {{mysharedariaselected}}>
                    {{#str}}navbar:tab:mysharedeportfolios, local_eportfolio{{/str}}
                </a>
            </li>
            <li class="nav-item" role="none">
                <a href="{{mygradeurl}}" title="{{#str}}navbar:tab:mysharedeportfoliosgrade, local_eportfolio{{/str}}"
                   class="nav-link {{mygradeactive}}"
                   role="menuitem" {{mygradeariaselected}}>
                    {{#str}}navbar:tab:mysharedeportfoliosgrade, local_eportfolio{{/str}}
                </a>
            </li>
            <li class="nav-item" role="none">
                <a href="{{sharedurlurl}}" title="{{#str}}navbar:tab:sharedeportfolios, local_eportfolio{{/str}}"
                   class="nav-link {{sharedactive}}"
                   role="menuitem" {{sharedariaselected}}>
                    {{#str}}navbar:tab:sharedeportfolios, local_eportfolio{{/str}}
                </a>
            </li>
            {{#sharedeportfoliosgrade}}
                <li class="nav-item" role="none">
                    <a href="{{gradeurl}}" title="{{#str}}navbar:tab:sharedeportfoliosgrade, local_eportfolio{{/str}}"
                       class="nav-link {{gradeactive}}"
                       role="menuitem" {{gradeariaselected}}>
                        {{#str}}navbar:tab:sharedeportfoliosgrade, local_eportfolio{{/str}}
                    </a>
                </li>
            {{/sharedeportfoliosgrade}}
            <li class="nav-item" role="none">
                <a href="{{templateurl}}" title="{{#str}}navbar:tab:sharedtemplates, local_eportfolio{{/str}}"
                   class="nav-link {{templateactive}}"
                   role="menuitem" {{templateariaselected}}>
                    {{#str}}navbar:tab:sharedtemplates, local_eportfolio{{/str}}
                </a>
            </li>
        </ul>
    </div>
</div>